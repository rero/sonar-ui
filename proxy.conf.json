{
  "/api/*": {
    "target": "https://localhost:5000",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": "https://localhost:5000/api"
    }
  },
  "/logged-user/": {
    "target": "https://localhost:5000",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": "https://localhost:5000/logged-user/"
    }
  }
}