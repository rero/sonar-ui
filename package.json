    "name": "RERO",
    "email": "software@rero.ch"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ng build sonar --prod",
    "deploy": "ng build sonar --prod  --deploy-url /static/sonar-ui/",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "pack": "npm run deploy && npm pack",
    "serve": "ng serve --configuration proxy",
    "test": "ng test",
    "extract_messages": "ngx-translate-extract -k -c -s -i ./projects/sonar/src -o ./projects/sonar/src/app/translations/i18n/messages.json --fi '  '",
    "update_catalog": "ngx-translate-extract -k -c -s -i ./projects/sonar/src -o ./projects/sonar/src/app/translations/i18n/{de,en,fr,it}.json --fi '  '"
  },
