    "minimist": ">=0.2.1",
    "moment": "^2.24.0",
    "ngx-bootstrap": "~5.3.2",
    "ngx-dropzone": "^2.1.1",
    "ngx-spinner": "^8.0.3",
    "ngx-toastr": "^10.2.0",
